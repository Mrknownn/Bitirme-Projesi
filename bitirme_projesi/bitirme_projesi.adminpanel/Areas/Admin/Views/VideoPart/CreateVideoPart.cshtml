@model CreateVideoPartDto
@{
    ViewData["Title"] = "CreateNews";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}
<h1>Yeni Video Girişi</h1>
<form method="post">
    <label>Video Başlığı</label>
    <input type="text" asp-for="videoPartName" class="form-control" />
    <br />
    <label>Video ID</label>
    <input type="textarea" asp-for="videoDataNumber" class="form-control" />
    <br />
    <label>Video Sahibi</label>
    <input type="text" asp-for="videoAuthor" class="form-control" />
    <br />
    <label>Video Link</label>
    <input type="text" asp-for="videoUrl" class="form-control" />
    <br />
    <button class="btn btn-primary">Haberi Ekle</button>
</form>

